{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "declaration": true,
    "declarationDir": "dist",
    "outDir": "dist",
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "(appLayout)/*": ["./(appLayout)/*"],
      "(fakePanther)/*": ["./(fakePanther)/*"],
      "(auth)/*": ["./(auth)/*"],
      "(grafana)/*": ["./(grafana)/*"],
      "(map)/*": ["./(map)/*"],
      "(shared)/*": ["./(shared)/*"]
    }
  },
  "include": ["**/*"],
  "exclude": ["node_modules", "dist"]
}
